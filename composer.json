{
    "name": "clumsy/utils",
    "description": "A set of utilities for building Laravel applications",
    "keywords": ["laravel", "helper", "utilities"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Buteler",
            "email": "tbuteler@gmail.com"
        }
    ],
    "require": {
        "ext-bcmath": "*",
        "lib-curl": "*",
        "laravel/framework": ">=5.1",
        "laravelcollective/html": ">=5.1",
        "geoip2/geoip2":"~2.0",
        "clumsy/assets": ">=0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "files": [
            "src/helpers.php",
            "src/deprecated.php"
        ],
        "psr-4": {
            "Clumsy\\Utils\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
